<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ch.snip_it">

    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_ch"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_ch"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.ch.snip_it.SelectContactActivity"
            android:label="SelectContactActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.example.ch.snip_it.users.CreateUserActivity"
            android:label="CreateUserActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.example.ch.snip_it.users.UserHomePage"
            android:label="UserHomePage"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.example.ch.snip_it.users.FriendActivity"
            android:label="FriendActivity"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>